{
  "name": "starkshield-tests",
  "version": "0.1.4",
  "description": "Integration tests for StarkShield",
  "scripts": {
    "test": "mocha --timeout 60000 \"*.test.js\"",
    "test:watch": "mocha --watch --timeout 60000 \"*.test.js\"",
    "test:ci": "mocha --timeout 120000 --reporter json \"*.test.js\" > test-results.json"
  },
  "dependencies": {
    "starknet": "^6.0.0",
    "ethers": "^6.0.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "mocha": "^10.2.0",
    "chai": "^4.3.0",
    "sinon": "^17.0.0"
  }
}
